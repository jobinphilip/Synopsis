<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.synopsis.androidapp.synopsis">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ldpi"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:name=".MyAnalytics"
        android:noHistory="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">

     <meta-data android:name="com.google.android.gms.version"
         android:value="@integer/google_play_services_version"></meta-data>
    <!--    <meta-data
            android:name="com.google.android.gms.analytics.globalConfigResource"
            android:resource="@xml/global_tracker" />

-->
        <activity android:name=".Start_Application">
            <intent-filter>

                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>


        </activity>

        <activity
            android:name=".BasicInfoClass"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:windowSoftInputMode="stateHidden"></activity>
        <activity
            android:name=".Dash_board"
            android:screenOrientation="portrait"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:windowSoftInputMode="stateHidden"></activity>
        <activity
            android:name=".LoginClass"
            android:noHistory="true"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:windowSoftInputMode="stateHidden"></activity>
        <activity
            android:name=".RegisterUserClass"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:windowSoftInputMode="stateHidden"></activity>
        <activity
            android:name=".Terms_conditionsClass"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:noHistory="true"
            android:windowSoftInputMode="stateHidden"></activity>
        <activity
            android:name=".Verified_referals_list"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:windowSoftInputMode="stateHidden"></activity>
        <activity
            android:name=".Referals_list"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:windowSoftInputMode="stateHidden"></activity>
        <activity
            android:name=".Forgot_password"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:windowSoftInputMode="stateHidden"></activity>
        <activity
            android:name=".Camera_activity"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:windowSoftInputMode="stateHidden"></activity>


        <activity
            android:name=".Forgot_password_ResetClass"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:windowSoftInputMode="stateHidden"></activity>



        <activity
            android:name=".Company_register"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:noHistory="true"
            android:windowSoftInputMode="stateHidden"></activity>

        <activity
            android:name=".Internet_ErrorMessage"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:noHistory="true"
            android:windowSoftInputMode="stateHidden"></activity>



    </application>

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

</manifest>


